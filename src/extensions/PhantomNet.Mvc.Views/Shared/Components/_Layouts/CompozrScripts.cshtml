@inject IHtmlLocalizer<CompozrResources> CompozrLocalizer

<script src="~/assets/rangy/rangy-core.js"></script>
<script src="~/assets/rangy/rangy-selectionsaverestore.js"></script>
<script src="~/assets/moment/min/moment-with-locales.js"></script>
<script src="~/assets/eonasdan-bootstrap-datetimepicker/src/js/bootstrap-datetimepicker.js"></script>
<script src="~/assets/select2/select2.js"></script>

<script src="~/assets/phantomnet-compozr-jquery/dist/js/phantomnet-compozr.js"></script>

@RenderBody()

<script>
        $(function () {
            Compozr.init($("#layout-compozr"), {
                closeConfirm: "@CompozrLocalizer["CloseConfirm"]",
                newError: "@CompozrLocalizer["NewError"]",
                validationError: "@CompozrLocalizer["ValidationError"]",
                saveError: "@CompozrLocalizer["SaveError"]",
                revertConfirm: "@CompozrLocalizer["RevertConfirm"]",
                deleteConfirm: "@CompozrLocalizer["DeleteConfirm"]",
                deleteError: "@CompozrLocalizer["DeleteError"]",
                unhandledError: "@CompozrLocalizer["UnhandledError"]"
            });
        });
</script>
